<Window x:Class="Launcher8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Launcher8.Views"
        mc:Ignorable="d"
        Background="#FF111111"
        Title="MainWindow" Width="400" Height="800" >
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <local:Apps x:Name="AppsPanel" VerticalAlignment="Stretch" HorizontalAlignment="Stretch"></local:Apps>
        <Image Grid.Row="1" x:Name="ShellButton" Height="64" HorizontalAlignment="Center"></Image>
    </Grid>
</Window>
