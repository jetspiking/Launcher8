<UserControl x:Class="Launcher8.Views.AppListing"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Launcher8.Views"
      mc:Ignorable="d"
      >

    <StackPanel Height="45" Margin="5,5,5,5" Name="Holder" Orientation="Horizontal" >
        <Image Name="Icon" Margin="5,0,5,0" Width="40" Height="40" HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
        <Label Name="Title" Margin="5,0,5,0" HorizontalAlignment="Left" VerticalAlignment="Center"></Label>
    </StackPanel>
</UserControl>
